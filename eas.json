{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "node": "22.11.0",
      "autoIncrement": true,
      "env": {
        "node": "22.11.0",
        "NODE_ENV": "production"
      },
      "ios": {
        "image": "latest",
        "cocoapods": "1.15.2"
      },
      "hooks": {
        "preUploadArtifact": {
          "path": "./scripts/eas-build-pre-upload-artifact.sh"
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascApiKeyPath": "./private/AuthKey_YY2KC425HU.p8",
        "ascApiKeyId": "YY2KC425HU",
        "ascApiKeyIssuerId": "283eb7e7-e049-4aae-afe2-d6873f45b981"
      },
      "android": {
        "serviceAccountKeyPath": "./private/service-account.json"
      }
    }
  }
}
